<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gps_cord.routes"
    android:versionCode="2"
    android:versionName="1.01" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity
            android:name="com.gps_cord.routes.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>

        <service android:name="com.gps_cord.routes.GPSService" >
            <intent-filter>
                <action android:name="com.example.gps1.action.LOG_POS" />
            </intent-filter>
        </service>

        <receiver android:name="com.gps_cord.routes.locationReceiver" >
            <intent-filter>
                <action android:name="com.example.gps1.action.LOCATION_UPDATED" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.gps_cord.routes.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gps_cord.routes.Test"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name="com.gps_cord.routes.MyList"
            android:label="@string/title_activity_list" >
        </activity>
    </application>

</manifest>
