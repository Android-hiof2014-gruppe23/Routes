<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gps_cord.routes"
    android:versionCode="2"
    android:versionName="1.01" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
	
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light" >
        <meta-data 
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCeoxriV_etpJ8ff4_eferfx4QPQAo3GXw"/>
        <meta-data
		    android:name="com.google.android.gms.version"
		    android:value="@integer/google_play_services_version" />
        <activity
            android:name="com.gps_cord.routes.GPSActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>

        <service android:name="com.gps_cord.routes.GPSService" >
            <intent-filter>
                <action android:name="com.example.gps1.action.LOG_POS" />
            </intent-filter>
        </service>

        <receiver android:name="com.gps_cord.routes.locationReceiver" >
            <intent-filter>
                <action android:name="com.example.gps1.action.LOCATION_UPDATED" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.gps_cord.routes.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gps_cord.routes.Test"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name="com.gps_cord.routes.ListOfActivities"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name="com.gps_cord.routes.DataOfActivity"
            android:label="@string/title_activity_data_of" >
        </activity>
        
    </application>

</manifest>
